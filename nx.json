{
  "npmScope": "tin",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "tin": {
      "tags": []
    },
    "tin-e2e": {
      "tags": [],
      "implicitDependencies": ["tin"]
    },
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "auth-domain": {
      "tags": ["domain:auth", "type:domain-logic"]
    },
    "auth-feature-login": {
      "tags": ["domain:auth", "type:feature"]
    },
    "auth-shell": {
      "tags": ["domain:auth", "type:shell"]
    },
    "shared-ui": {
      "tags": ["domain:shared", "type:ui"]
    }
  }
}
